{
  "name": "trading-dashboard",
  "version": "1.0.0",
  "description": "Professional Trading Dashboard for Institutional-Grade Algorithmic Trading Platform",
  "type": "module",
  "scripts": {
    "start": "vite --host 0.0.0.0 --port 3003",
    "dev": "vite --host 0.0.0.0 --port 3003",
    "dev-no-check": "vite --host 0.0.0.0 --port 3003",
    "build": "tsc && vite build",
    "build-no-check": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "start:full": "concurrently \"python ../backend_api_server.py\" \"npm run dev\"",
    "start:backend": "python ../backend_api_server.py",
    "install:backend": "python ../install_backend_deps.py"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.5",
    "@mui/icons-material": "^5.14.3",
    "@mui/x-charts": "^6.0.0",
    "@mui/x-data-grid": "^6.10.2",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.2",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "zustand": "^4.4.1",
    "react-query": "^3.39.3",
    "framer-motion": "^10.16.1",
    "react-hot-toast": "^2.4.1",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/lodash": "^4.14.196",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "keywords": [
    "trading",
    "algorithmic-trading",
    "dashboard",
    "react",
    "typescript",
    "real-time",
    "websocket",
    "institutional"
  ],
  "author": "Intraday Trading Platform",
  "license": "MIT"
}
